    <!-- Users form view -->
    <record id="res_users_form_calendar" model="ir.ui.view">
        <field name="name">res.users.form.calendar</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="mail.view_users_form_mail"/>
